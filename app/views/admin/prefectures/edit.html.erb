<div id="main">
  <p><%= link_to("管理画面TOPに戻る", admin_home_index_path ) %></p>

	<section>
		<%= form_with(model: @prefecture, url: admin_prefecture_path, method: :PATCH) do |f| %>
			<table class="ta1">
				<tr>
					<th>都道府県</th>
					<td><%= f.text_area :prefecture, size: "10", class:"wl" %></td>
				</tr>
			</table>
			<p class="c">
				<%= f.submit "編集内容を確認する", class:"btn" %>
			</p>
		<% end %>
	</section>

</div>
