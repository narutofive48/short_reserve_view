<div id="main">

	<section>

		<%= form_with(model: @reserve, url: delete_reserf_path, method: :POST) do |f| %>
			<table class="ta1">
			  <div class="button-modoru-box">
          <%= link_to "戻る", 'javascript:history.back()', method: :get, class: "button-modoru" %>
        </div>
				<tr>
					<th>ケアマネジャー</th>
					<td><%= f.hidden_field :user_id %><%= @reserve.uaccount.user_name %></td>
				</tr>
				<tr>
					<th>ショートステイ事業所番号</th>
					<td><%= f.hidden_field :office_id %><%= @reserve.oaccount.office_name %></td>
				</tr>
				<tr>
					<th>利用者名</th>
					<td><%= f.hidden_field :client_name %><%= @reserve.client_name %></td>
				</tr>
				<tr>
					<th>開始日</th>
					<td><%= f.hidden_field :start_date %><%= @reserve.start_date %></td>
				</tr>
				<tr>
					<th>迎えの送迎</th>
					<td><%= f.hidden_field :start_transfer_id %><%= @reserve.start_transfer.trans_flg %></td>
				</tr>
				<tr>
					<th>終了日</th>
					<td><%= f.hidden_field :end_date %><%= @reserve.end_date %></td>
				</tr>

				<tr>
					<th>送りの送迎</th>
					<td><%= f.hidden_field :end_transfer_id %><%= @reserve.end_transfer.trans_flg %></td>
				</tr>
				<tr>
					<th>過去に利用がありますか？</th>
					<td><%= f.hidden_field :past_use_id %><%= @reserve.past_use.past_use %></td>
				</tr>
				<tr>
					<th>備考欄</th>
					<td><%= f.hidden_field :remark %><%= @reserve.remark %></td>
				</tr>
			</table>

			<p class="c">
				<%= f.submit "この予約を削除する", class:"btn" %>
			</p>
		<% end %>
	</section>

</div>
<!--/#main-->
